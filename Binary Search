#include<stdio.h>
int binary_search(int arr[],int x,int low,int high)
{
    if(high>=low)
    {
        int mid=(low+high)/2;
        if(arr[mid]=x)
        return mid;
        else if(arr[mid]>x)
        return binary_search(arr,x,low,mid-1);
        else 
        return binary_search(arr,x,mid+1,high);
    }
    return -1;
}
int main()
{
    int i,x,n;
    printf("Enter the no of elements in array\n");
    scanf("%d",&n);
    int arr[n];
    printf("Enter the array elements in ascending order\n");
    for(i=0;i<n;i++)
    {
        scanf("%d",&arr[i]);
    }
    printf("Enter the element to search:");
    scanf("%d",&x);
    int result=binary_search(arr,x,0,n-1);
    if(result==-1)
    printf("Not found");
    else
    printf("Element found at %d",result+1);
}
